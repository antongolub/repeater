{
  "presets": [
    "flow",
    "env"
  ],
  "plugins": [
    "transform-class-properties"
  ],
  "comments": false,
  "ignore": [
    "interface.js"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-class-properties"
      ],
      "presets": [
        "flow",
        ["env", {
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }],
        "minify"
      ]
    }
  }
}